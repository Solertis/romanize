{
  "name": "romanize",
  "description": "Convert arabic numbers to roman numerals (useful for books, outlines, documentation, slide decks, etc)",
  "version": "0.1.1",
  "homepage": "https://github.com/jonschlinkert/romanize",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/romanize",
  "bugs": {
    "url": "https://github.com/jonschlinkert/romanize/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "devDependencies": {
    "mocha": "*"
  },
  "keywords": [
    "arabic",
    "number",
    "numbers",
    "numeral",
    "numerals",
    "roman"
  ],
  "verb": {
    "related": {
      "list": [
        "deromanize"
      ]
    },
    "plugins": [
      "gulp-format-md"
    ],
    "toc": false,
    "layout": "default"
  }
}
